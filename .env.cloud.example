# OMEGA Cloud Nexus v4.0 - Environment Variables Template
# Inject via: fly secrets set KEY=VALUE
# DO NOT commit real secrets to Git.

# ─── Auth ─────────────────────────────────────────────────
TELEGRAM_BOT_TOKEN=
OPENCLAW_GATEWAY_TOKEN=
# Generator: openssl rand -hex 32

# ─── Model Providers ─────────────────────────────────────
GOOGLE_API_KEY=
OPENAI_API_KEY=

# ─── Runtime Config ──────────────────────────────────────
NODE_ENV=production
OPENCLAW_STATE_DIR=/data
NODE_OPTIONS="--max-old-space-size=180"
PROXY_PORT=8001
DEFAULT_GEMINI_MODEL=gemini-2.0-flash
